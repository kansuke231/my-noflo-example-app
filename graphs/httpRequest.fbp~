'4343' -> LISTEN Server(webserver/Server) REQUEST -> IN BodyParser(webserver/BodyParser)
BodyParser() OUT -> IN  Sanitizer(Sanitizer) OUT -> IN Display(core/Output)

Server() REQUEST -> IN WriteResponse(webserver/WriteResponse)
WriteResponse() OUT -> IN SendResponse(webserver/SendResponse)

